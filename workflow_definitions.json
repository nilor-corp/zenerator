    "upscale-video":{
        "name": "Upscale Video",
        "filename": "upscale-video.json",
        "description": "Upscale a video.",
        "inputs": {
            "video-to-uscale-path": {
                "type": "video",
                "label": "Video to Upscale",
                "node-id": "[\"419\"][\"inputs\"][\"string\"]",
