                    }
                }
            },

            "upscale-by": {
                "type": "float",
                "label": "Upscale By",
                "node-id": "[\"422\"][\"inputs\"][\"float\"]",
                "value": 2.5,
                "minimum": 1.0,
                "maximum": 4.0,
                "step": 0.25
